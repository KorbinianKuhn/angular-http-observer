    "ng-packagr": "^4.3.1",
    "rxjs": "^6.3.3",
    "tsickle": "^0.33.0",
    "typescript": "^3.1.3",
    "zone.js": "^0.8.26"
  },
  "author": "Korbinian Kuhn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KorbinianKuhn/angular-http-observer/issues"
